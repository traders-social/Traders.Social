<div class="container-fluid container-compound-form" id="register_form">
    <div class="row">
        <div class="col col-left">{% include '/forms/user/userbase.html.twig' with {'user': form.user} %}</div>
        <div class="col-md-6 col-xl-6 col-image"  style="background-image: url('{{ asset('assets/images/forms/register/register-1.jpg') }}');"></div>
    </div>
    <div class="row">
        <div class="col-md-6 col-xl-6 col-image"  style="background-image: url('{{ asset('assets/images/forms/register/register-2.jpg') }}');"></div>
        <div class="col col-right">{% include '/forms/user/info.html.twig' with {'userinfo': form.userinfo} %}</div>
    </div>
    <div class="row">
        <div class="col">
            <div class="container">
                {% include '/forms/user/addressbook.html.twig' with {'addresses': form.addresses} %}
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <div class="container">
                {{ form_row(form.register) }}
            </div>
        </div>
    </div>

    {{ form_row(form.uuid) }}
    {{ form_row(form._token) }}
</div>

{% block javascripts %}
    {{ encore_entry_script_tags('register') }}
{% endblock %}
