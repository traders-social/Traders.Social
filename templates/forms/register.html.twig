<div class="container-fluid container-compound-form" id="register_form" style="background-image: url('{{ asset('images/forms/register-2.jpg') }}');">
    <div class="container-form container-fluid">
        <div class="row">
            <div class="col">{% include '/forms/user/userbase.html.twig' with {'user': form.user} %}</div>
            <div class="col-md-6 col-xl-6"></div>
        </div>
        <div class="row">

            <div class="col">{% include '/forms/user/info.html.twig' with {'userinfo': form.userinfo} %}</div>
            <div class="col-md-6 col-xl-6"></div>
        </div>
        <div class="row">
            <div class="col">{% include '/forms/user/addressbook.html.twig' with {'addresses': form.addresses} %}</div>
        </div>
        <div class="row">
            <div class="col">
                {{ form_row(form.register) }}
            </div>
        </div>
    </div>
    {{ form_row(form.uuid) }}
</div>
