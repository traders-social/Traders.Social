{% extends '/base.html.twig' %}

{% block title %}Create an account{% endblock %}

{% block tagline %}Register to become a trader now!{% endblock %}

{% block body %}
    <div class="wrapper">
        <div class="register-eyecatcher"></div>
        <div class="register-content">
            <div class="container-fluid container-errors">
                {{ form_errors(form) }}
            </div>
            {{ form_start(form) }}
            {% include '/forms/register.html.twig' with { 'form': form } %}
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}
